{
  "expo": {
    "name": "Payvo Middleware",
    "slug": "payvo-middleware",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/app_icon.png",
    "userInterfaceStyle": "light",
    "scheme": "ai.payvo.middleware",
    "splash": {
      "image": "./images/app_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ai.payvo.middleware",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/app_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "ai.payvo.middleware",
      "versionCode": 1
    },
    "web": {
      "favicon": "./images/app_icon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "owner": "payvo-ai",
    "extra": {
      "eas": {
        "projectId": "1ddab57f-3fcf-4b02-89f5-28f727703330"
      }
    }
  }
}
